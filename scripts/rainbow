y=0
while read line; do
    for ((x=0; x<${#line}; x++)); do
        index=$(((x*8 + y*16)%$colors_num))
        rgb=${colors[$index]}
        echo -en "$rgb${line:$x:1}$reset"
    done
    ((y++))
    echo ""
done